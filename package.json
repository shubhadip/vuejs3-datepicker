  "repository": {
    "type": "git",
    "url": "git+https://github.com/shubhadip/vuejs3-datepicker.git"
  },
  "bugs": {
    "url": "https://github.com/shubhadip/vuejs3-datepicker/issues"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
